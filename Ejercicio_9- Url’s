import re

regex = r"[https?:\/\/]+([w]{3})?\w+[.?].+"

test_str = ("Ejemplos de URL's\n"
	"https://github.com/cecilioct/ExpersionesRegulares.git\n"
	"https://ejemplos.net/10-ejemplos-de-correos-electronicos/\n"
	"https://www.comunicamedia.com/correos-electronicos-efectivos/\n"
	"https://computerhoy.com/reportajes/tecnologia/inteligencia-artificial-469917\n"
	"https://www.oracle.com/mx/artificial-intelligence/what-is-artificial-intelligence.html\n"
	"https://moodle.valladolid.tecnm.mx/my/\n"
	"Inteligencia artificial: qué es, cómo funciona y para qué se está utilizando.\n")

matches = re.finditer(regex, test_str, re.MULTILINE)

for matchNum, match in enumerate(matches, start=1):
    
    print ("Match {matchNum} was found at {start}-{end}: {match}".format(matchNum = matchNum, start = match.start(), end = match.end(), match = match.group()))
    
    for groupNum in range(0, len(match.groups())):
        groupNum = groupNum + 1
        
        print ("Group {groupNum} found at {start}-{end}: {group}".format(groupNum = groupNum, start = match.start(groupNum), end = match.end(groupNum), group = match.group(groupNum)))
